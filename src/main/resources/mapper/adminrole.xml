<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cykj.net.mapper.AdminRoleDao">
    <!--单条数据插入，返回自增ID序号-->
    <insert id="regAdminRole" parameterType="Adminrole" keyProperty="arid" useGeneratedKeys="true">
		insert into
		adminrole(account,roid) values(#{account},#{roid})
	</insert>

</mapper>